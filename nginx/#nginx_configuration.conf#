events{}
http {
    include      mime.types;
    default_type application/octet-stream;
    merge_slashes on;

    server {
        listen 80 ssl;
        server_name c;
        
        location / {
            root   /app;
            index  index.html;
            try_files $uri $uri/ =404;
        }
    }
}Sv1.3;
        ssl_ciphers   HIGH:!aNULL:!MD5;

        location / {
            root   /app;
            index  index.html;
            try_files $uri $uri/ =404;
        }
    }
}